//Variavel para pegar o valor aleatorio
const random = Math.floor(Math.random() * image_random.length);
let count = 1;

//Contador
setInterval( function(){
    nextImage();
}, 2000)

function nextImage() {
    count++;
    if(count > 1){
    random, image_random[random];
    }
}

